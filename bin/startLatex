#! /usr/bin/env python

from __future__ import print_function
import os
import sys

template = os.getenv('DATA_DIR') + '/tex_template.tex'

if len(sys.argv) >= 2:
    newfile = sys.argv[1]
else:
    print('Usage: startLatex filename')
    sys.exit(1)

assert os.path.isfile(template)

with open(template, 'r') as f:
    text = f.read()

with open(newfile, 'w') as f:
    f.write(text)
